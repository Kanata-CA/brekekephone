{
  "name": "brekekephone",
  "version": "2.6.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "craco start",
    "rn": "react-native start",
    "android": "react-native run-android",
    "android:shake": "adb shell input keyevent 82",
    "ios": "react-native run-ios",
    "ios:bitcode": "cd node_modules/react-native-webrtc/tools && bash downloadBitcode.sh",
    "format": "eslint --ignore-path=.gitignore --ext=.js,.ts,.tsx --fix . && prettier --ignore-path=.gitignore --loglevel=error --write \"**/*.{html,js,ts,tsx,json,md}\"",
    "format:check": "eslint --ignore-path=.gitignore --ext=.js,.ts,.tsx --max-warnings=0 . && prettier --ignore-path=.gitignore --list-different \"**/*.{js,ts,tsx,html,css,scss,json,md,yml}\"",
    "ts": "tsc && type-coverage",
    "build": "del-cli -f \"node_modules/.cache/babel*\" && craco build",
    "build:intl": "del-cli -f \".babelPluginIntl.json\" && cross-env EXTRACT_INTL=1 yarn build"
  },
  "dependencies": {
    "@callstack/async-storage": "2.0.3",
    "@mdi/js": "5.8.55",
    "@react-native-community/async-storage": "1.6.3",
    "@react-native-community/push-notification-ios": "1.7.1",
    "brekekejs": "github:brekekesoftware/brekekejs#2274f2c10a09e4b8b55b7ae7bc2625643c5258e5",
    "buffer": "6.0.1",
    "circular-json": "0.5.9",
    "es6-shim": "0.35.6",
    "es7-shim": "6.0.0",
    "eventemitter3": "4.0.7",
    "filesize": "6.1.0",
    "handlebars": "4.7.6",
    "helper-moment": "0.2.0",
    "json-stable-stringify": "1.0.1",
    "jssip": "3.2.15",
    "lodash": "4.17.20",
    "mobx": "5.15.7",
    "mobx-react": "6.3.1",
    "moment": "2.29.1",
    "polished": "4.0.3",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "17.0.1",
    "react-art": "17.0.1",
    "react-device-detect": "1.14.0",
    "react-dom": "17.0.1",
    "react-native": "0.63.3",
    "react-native-callkeep": "github:brekekesoftware/react-native-callkeep#3dac522e1cae0d776b3ede42fea33976405b6041",
    "react-native-document-picker": "4.1.0",
    "react-native-emoji-selector": "0.1.9",
    "react-native-exception-handler": "2.10.9",
    "react-native-fast-image": "8.3.3",
    "react-native-fcm": "github:brekekesoftware/react-native-fcm#afbddd1782afe9a6cf81c34f903e026b712d5579",
    "react-native-fs": "2.16.6",
    "react-native-hyperlink": "0.0.19",
    "react-native-image-picker": "2.3.4",
    "react-native-incall-manager": "3.3.0",
    "react-native-iphone-x-helper": "1.3.1",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-linear-gradient": "2.5.6",
    "react-native-share": "4.1.0",
    "react-native-splash-screen": "github:brekekesoftware/react-native-splash-screen#a78ee51c0f89652a2a223ff2bc4ec10928bf2c30",
    "react-native-svg": "12.1.0",
    "react-native-svg-web": "1.0.9",
    "react-native-uuid": "1.4.9",
    "react-native-voip-push-notification": "2.1.0",
    "react-native-web": "0.14.7",
    "react-native-web-linear-gradient": "1.1.1",
    "react-native-webrtc": "1.75.3",
    "url-parse": "1.4.7",
    "uuid": "8.3.1",
    "validatorjs": "3.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.1",
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/plugin-transform-react-jsx": "7.12.5",
    "@craco/craco": "5.8.0",
    "@prettier/plugin-xml": "0.12.0",
    "@types/circular-json": "0.4.0",
    "@types/handlebars": "4.1.0",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jssip": "3.3.1",
    "@types/lodash": "4.14.165",
    "@types/md5": "2.2.1",
    "@types/qs": "6.9.5",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-native": "0.63.33",
    "@types/react-native-incall-manager": "3.2.0",
    "@types/react-native-keyboard-spacer": "0.4.1",
    "@types/react-native-share": "3.3.0",
    "@types/react-native-webrtc": "1.75.2",
    "@types/url-parse": "1.4.3",
    "@types/validatorjs": "3.15.0",
    "circular-dependency-plugin": "5.2.2",
    "cross-env": "7.0.2",
    "del-cli": "3.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "fs-extra": "9.0.1",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "md5": "2.3.0",
    "prettier": "2.1.2",
    "react-scripts": "3.4.3",
    "type-coverage": "2.14.2",
    "typescript": "4.0.5"
  },
  "typeCoverage": {
    "atLeast": 100,
    "ignoreCatch": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,scss,json,md}": "prettier --write"
  },
  "browserslist": "ie 10-11"
}
